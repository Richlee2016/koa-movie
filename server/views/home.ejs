<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <%- include("layout/jsCss") -%>
</head>

<body>
  <!-- 公用header -->
  <%- include("layout/header") -%>
    <section id="container" class="container">
      <div class="main">
        <ul class="newest">
          <% banner.forEach(function(o,i){ %>
            <li class="vivify animationObject popInTop">
              <a href="movie/<%= o.id %>">
                <img src=<%= o.img %> alt="">
                <span class="black-block"><%= o.year %></span>
                <span class="black-block"><%= o.score %></span>
                <p class="black-block"><%= o.title %></p>
              </a>
            </li>
          <% }) %>  
        </ul>
      </div>
    </section>
    <!-- main JS -->
    <%- include("layout/mainScript",{name:'home'}) -%>
</body>
</html>
